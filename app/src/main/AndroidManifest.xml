<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.htcdiurno.a1ud1pmdm">

    <!--Aquí indicamos al manifest el nombre de la aplicación, su icono, el estilo, si se puede hacer backup...-->
    <application
        android:allowBackup="true"
        android:icon="@mipmap/jrjc"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/jrjc"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <!--Indicamos qué activities contendrá la aplicación. En este caso solo la activity principal.-->
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>